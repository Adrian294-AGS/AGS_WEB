<form action="/songInsert" method="post" enctype="multipart/form-data">
    <label for="songName">Song Name</label>
    <input id="songName" type="text" name="songName">

    <label for="songFile">Song File</label>
    <input type="file" name="songFile" id="songFile" accept="image/*">

    <label for="songImage">Song Image</label>
    <input type="file" name="songImage" id="songImage" accept="audio/*">

    <button type="submit">submit</button>
</form>